target 'SettingsBundle' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SettingsBundle
  pod 'Alamofire'
  
  plugin 'cocoapods-acknowledgements', :settings_bundle => true

end
